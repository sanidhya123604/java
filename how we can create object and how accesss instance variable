// how we create the object and how we access the instance variable
public class Student {
	int rollNo ;   // instance variable
	String name;
	void setData(int x, String y)
	{
		rollNo = x;
		name = y;
		
	}
	void display()
	{
		System.out.println("student roll number:"+rollNo);
		System.out.println("student name :"+name);
	}
	

	public static void main(String[] args) {
        Student s1 = new Student();        // object creation for class
		s1.setData(123,"sanidhya");        // access ther method through object
		s1.display();
	}

}
