// program of constructor using the this keyword
public class Main
{
    int roll;
    String name;
    Main(int roll, String name)
    {
        this.roll = roll;
        this.name = name;
    }
    void displayData()
    {
        System.out.println("roll=" +roll);
        System.out.println("name=" +name);
    }
    
	public static void main(String[] args) {
		Main s1 = new Main(100,"sourav");
		Main s2 = new Main(101,"sakshi");
		s1.displayData();
		s2.displayData();
	}
}

