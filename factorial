import java.util.*;
public class Main
{
    public static int factorial(int num)
    {
        int sum =1;
        for(int i=1;i<=num;i++)
        {
            sum = sum*i;
        }
        return sum;
    }
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    int  num = sc.nextInt();
	    int d = factorial(num);
	    System.out.println(d);
	}
}
