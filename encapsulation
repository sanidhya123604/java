// program of Incapsulation
public class Main
{
    private int roll;
    private String name;
    public  void setData(int roll,String name)
    {
        this.roll= roll;
        this.name=name;
    }
    public void getData()
    {
        System.out.println("roll"+roll);
        System.out.println("name"+name);
    }
	public static void main(String[] args) {
	    Main s = new Main();
	    s.setData(101,"sourav");
	    s.getData();
	}
}
